<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web mobile cloud testing Suite for iOS">
    <parameter name="useCloud" value="true" />
    <parameter name="appType" value="web" />
    <parameter name="udid" value="" />
    <parameter name="platformName" value="iOS"/>
    <parameter name="browserName" value="Safari"/>
    <test name="Web cloud tests for iOS">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
