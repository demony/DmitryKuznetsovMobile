<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Native mobile cloud testing Suite for iOS">
    <parameter name="useCloud" value="true" />
    <parameter name="appType" value="native" />
    <parameter name="udid" value="" />
    <parameter name="platformName" value="iOS"/>
    <parameter name="bundleId" value="com.epam.EPAMTestApp.ii"/>
    <test name="Native cloud tests for iOS">
        <groups>
            <run>
                <include name="native"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
