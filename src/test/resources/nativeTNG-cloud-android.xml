<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Native mobile cloud testing Suite for Android">
    <parameter name="useCloud" value="true" />
    <parameter name="appType" value="native" />
    <parameter name="udid" value="" />
    <parameter name="platformName" value="Android" />
    <test name="Native cloud tests for Android">
        <groups>
            <run>
                <include name="native"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
